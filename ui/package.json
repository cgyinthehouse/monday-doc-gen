{
  "name": "docgen-ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "pnpm dlx http-server -p 8301 build",
    "preview": "vite preview --port 8301",
    "dev": "npm run stop && concurrently \"npm run server\" \"npm run expose\"",
    "build": "vite build",
    "expose": "mapps tunnel:create -p 8301 -a 10226762",
    "server": "vite",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "stop": "kill-port 8301 && kill-port 4049 && kill-port 4040"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@mondaycom/apps-sdk": "^2.1.2",
    "@vibe/core": "^3.1.0",
    "@vibe/icons": "^1.0.0",
    "graphql": "^16.9.0",
    "monday-sdk-js": "^0.5.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.7.8",
    "@types/react": "^18.3.11",
    "@types/react-dates": "^21.8.6",
    "@types/react-dom": "^18.3.1",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^4.3.2",
    "concurrently": "^5.2.0",
    "cross-port-killer": "^1.2.1",
    "eslint-config-react-app": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "react-select": "^3.2.0",
    "sass": "^1.54.8",
    "typescript": "^5.6.3",
    "vite": "^5.4.9"
  }
}